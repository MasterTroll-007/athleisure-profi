<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude all shared preferences including encrypted ones -->
        <exclude domain="sharedpref" path="."/>
        <!-- Exclude datastore files -->
        <exclude domain="file" path="datastore/"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude all shared preferences including encrypted ones -->
        <exclude domain="sharedpref" path="."/>
        <!-- Exclude datastore files -->
        <exclude domain="file" path="datastore/"/>
    </device-transfer>
</data-extraction-rules>
